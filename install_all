#!/bin/bash

cd /Users/piaverous/Documents/DTY/Projet_Thales/thales-project/

cd ml_tools
source py36_ml_env/bin/activate
pip install -r requirements.txt
deactivate

cd ../back_django
source py36_back_env/bin/activate
